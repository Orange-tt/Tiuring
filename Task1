import java.util.Scanner;
// TASK 1
class Main 
{
    public static void main(String[] args) 
{
        Scanner in = new Scanner(System.in);
        
        String s = in.next()+'\n';
        int n = in.nextInt();
        
        for(int i = 0; i<n; i++)
            System.out.print(s);
        
        /* // Альтернатива. Избегает большого количества команд в стеке, засчёт больших затрат памяти:
        String res = "";
        
        for(int i = 0; i<n;i++)
            res+=s;
            
        System.out.print(res)
        */
    }
}
